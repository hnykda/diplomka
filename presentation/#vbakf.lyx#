\end_layout

\begin_layout Itemize
např.
 faktorizovatelné distribuce 
\begin_inset Formula $\prob qZ=\prod_{i=1}^{M}\prob{q_{i}}Z$
\end_inset


\end_layout

\end_deeper
\begin_layout Frame
\begin_inset Argument 4
status open

\begin_layout Plain Layout
Optimalizační problém
\end_layout

\end_inset


\end_layout

\begin_layout Frame

\end_layout

\begin_deeper
\begin_layout Itemize
vzdálenost měříme pomocí Kullback-Leiblerovy divergence
\begin_inset Formula 
\[
\kl qp=-\int\prob qZ\ln\frac{\probcond pZX}{\prob qZ}\D{\Var Z}
\]

\end_inset


\end_layout

\begin_layout Itemize
optimalizační problém 
\begin_inset Formula 
\[
\prob qZ=\underset{\prob qZ\text{-fakt.}}{\text{argmin}}\kl{\prob qZ}{\probcond pZX}
\]

\end_inset


\end_layout

\begin_layout Itemize
stále nejde přímo, ekvivalentní maximalizaci 
\begin_inset Formula 
\[
\lueb q=\int\prob qZ\ln\frac{\probjoint pXZ}{\prob qZ}\D{\Var Z}
\]

\end_inset


\end_layout

\end_deeper

\begin_deeper
\begin_layout Itemize
odhady odpovídají
\begin_inset Formula 
\begin{align*}
p(\vec{z}_{k}|\vec{x}_{k}) & =N(\vec{z}_{k};\mat H_{k}\vec{x}_{k},\mathbf{R}_{k})\\
p(\vec{x}_{k}|\vec{x}_{k-1}) & =N(\vec{x}_{k};F_{k|k-1}\vec{x}_{k-1},\mathbf{Q}_{k}).
\end{align*}

\end_inset


\end_layout

\begin_layout Itemize
cílem je odhadnout kovariační matice šumů: 
\begin_inset Formula $\mat R_{k},\text{\mat Q}_{k}$
\end_inset


\end_layout

\begin_layout Itemize
konjugovaná apriorna je Inverzní Wishartova distribuce
\end_layout

\begin_layout Itemize
předpokládáme, že distribuce je faktorizovatelná
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
note{Aproximace!}
\end_layout

\end_inset


\begin_inset Formula 
\begin{align*}
p\big(\Xi,\mathbf{z}_{1:k}\big) & =\mathrm{N}\big(\mathbf{z}_{k};\mathbf{H}_{k}\mathbf{x}_{k},\mathbf{R}_{k}\big)\mathrm{N}\big(\mathbf{x}_{k};\hat{\mathbf{x}}_{k|k-1},\mathbf{P}_{k|k-1}\big)\\
 & \times\mathrm{IW}\big(\mathbf{P}_{k|k-1};\hat{t}_{k|k-1},\hat{\mathbf{T}}_{k|k-1}\big)\mathrm{IW}\big(\mathbf{R}_{k};\hat{u}_{k|k-1},\hat{\mathbf{U}}_{k|k-1}\big)\\
 & \times p\big(\mathbf{z}_{1:k-1}\big)
\end{align*}

\end_inset


\end_layout

